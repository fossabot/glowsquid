{
  "root": "apps/oxidize",
  "projectType": "application",
  "sourceRoot": "apps/oxidize/src",
  "targets": {
    "build": {
      "executor": "@nxrs/cargo:build",
      "options": {
        "release": false,
        "targetDir": "../../dist/apps/oxide/target",
        "outDir": "../../dist/apps/oxide/builds/debug"
      },
      "configurations": {
        "production": {
          "release": true,
          "targetDir": "../../dist/apps/oxide/target",
          "outDir": "../../dist/apps/oxide/builds/release"
        }
      }
    },
    "test": {
      "executor": "@nxrs/cargo:test",
      "options": {}
    },
    "lint": {
      "executor": "@nxrs/cargo:clippy",
      "options": {
        "fix": false,
        "failOnWarnings": true,
        "noDeps": true
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "yarn tauri dev",
        "cwd": "apps/oxidize"
      }
    }
  },
  "tags": [],
  "nx": {
    "implicitDependencies": ["tnt"]
  }
}
